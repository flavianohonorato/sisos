<!doctype html>
<html>
<head>
	<title>Service Order</title>
	<%= stylesheet_link_tag 	"application", :media => "all" %>
	<%= javascript_include_tag  "application" %>
	<%= csrf_meta_tags %>
</head>
<body>

	<% flash.each do |type, message| %>
	  <div id="flash" class="<%= type %>">
		<% message %>
	  </div>	
	<% end %>
	
<%= yield %>

</body>
</html>